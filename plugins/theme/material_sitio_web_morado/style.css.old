.image.img_01{
    top: 30px;
    left: 150px;
    -webkit-animation-name: imageUno;
    -webkit-animation-duration: 10s;
    -webkit-animation-iteration-count: infinite;
    animation-name: imageUno;
    animation-duration: 10s;
    animation-iteration-count: infinite;
}
@-webkit-keyframes imageUno {
    0%   {transform: translateY(-15px) rotate(-3deg);}
    25%  {transform: translateY(0px) rotate(3deg);}
    50%  {transform: translateY(-15px) rotate(-3deg);}
    75%  {transform: translateY(0px) rotate(3deg);}
    100% {transform: translateY(-15px) rotate(-3deg);}
}
@keyframes imageUno {
    0%   {transform: translateY(-15px) rotate(-3deg);}
    25%  {transform: translateY(0px) rotate(3deg);}
    50%  {transform: translateY(-15px) rotate(-3deg);}
    75%  {transform: translateY(0px) rotate(3deg);}
    100% {transform: translateY(-15px) rotate(-3deg);}
}
.image.img_02{
    top: 70px;
    right: 110px;
    -webkit-animation-name: imageDos;
    -webkit-animation-duration: 13s;
    -webkit-animation-iteration-count: infinite;
    animation-name: imageDos;
    animation-duration: 13s;
    animation-iteration-count: infinite;
}
@-webkit-keyframes imageDos {
    0%   {transform: translateY(-20px) rotate(-4deg);}
    25%  {transform: translateY(0px) rotate(0deg);}
    50%  {transform: translateY(-20px) rotate(-4deg);}
    75%  {transform: translateY(0px) rotate(0deg);}
    100% {transform: translateY(-20px) rotate(-4deg);}
}
@keyframes imageDos {
    0%   {transform: translateY(-20px) rotate(-4deg);}
    25%  {transform: translateY(0px) rotate(0deg);}
    50%  {transform: translateY(-20px) rotate(-4deg);}
    75%  {transform: translateY(0px) rotate(0deg);}
    100% {transform: translateY(-20px) rotate(-4deg);}
}
.image.img_03{
