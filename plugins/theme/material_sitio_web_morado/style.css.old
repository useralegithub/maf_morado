  src: url('fonts/Stanley-Bold.eot');
  src: url('fonts/Stanley-Bold.eot?#iefix') format('embedded-opentype'),
    url('fonts/Stanley-Bold.woff') format('woff'),
    url('fonts/Stanley-Bold.ttf')  format('truetype');
  font-weight: normal;
  font-style: normal;
}

@font-face {
