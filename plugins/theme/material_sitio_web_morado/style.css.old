html, body{
    margin:0;
    padding:0;
    width: 100%;
    height: 100%;
    font-size: 14px;
    font-family: 'AkkuratPro';
    font-weight: bold;
    background-color: #fffed7;
    overflow: hidden;
}




a{
    text-decoration: none;
    color: #0000a5;
}

.clear{
    clear: both;
}

::-moz-selection {
background: #fffed7;
color: #0000a5;
}
::selection {
background: #fffed7;
color: #0000a5;
}

.line{
    position: fixed;
    background-color: #0000a5;
    z-index: 5;
}

.line.line_l{
    height: 100%;
    width: 15px;
    top: 0;
    left: 0;
}
.line.line_r{
    height: 100%;
    width: 15px;
    top: 0;
    right: 0;
}
.line.line_t{
    width: 100%;
    left: 0;
    top: 0;
    height: 15px;
}
.line.line_b{
    width: 100%;
    height: 15px;
    bottom: 0;
    left: 0;
}

/*header*/
header{
    top: 15px;
    position: fixed;
    width: 100%;
    text-align: center;
    color: #0000a5;
    z-index: 5;
    left: 0;
    height: 0;
}

.header_int{
    height: 0;
    cursor: pointer;
}

.btn_menu{
    text-transform: uppercase;
    font-size: 20px;
    position: relative;
    display: table;
    margin: auto;
    padding: 0px 40px;
    border: 3px solid #0000a5;
    border-top: none;
    background-color: #fffcd7;
    height: 37px;
    line-height: 37px;
    transition: top 300ms;
    top: 0px;
    color: #fffcd7;
    background-color: #0000a5;
    border-bottom-left-radius: 20px;
    border-bottom-right-radius: 20px;
    font-weight: normal;
    font-size: 18px;
    letter-spacing: 2px;
}

.menu{
     /* display: none; */
     z-index: 2;
     position: fixed;
     top: 0px;
     left: 0px;
     width: 100%;
     height: auto;
     background-color: rgb(0, 0, 165);
     height: 100%;
}

.menu.act{
}

.btn_menu_cerrar{
    margin-top: 50px;
    text-transform: uppercase;
    font-size: 20px;
    text-align: right;
    color: #0000a5;
    padding-right: 45px;
    cursor: pointer;
}

.menu_int{
    position: relative;
    height: auto;
    color: #fffed7;
    width: 1020px;
    margin: auto;
    text-align: center;
}

.menu_int ul{
        margin: 20px 0px 30px;
